{
  "name": "typescript-swc-starter",
  "type": "module",
  "version": "1.2.3",
  "author": "maxgfr",
  "license": "MIT",
  "description": "A minimalist typescript swc starter",
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=24"
  },
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxgfr/typescript-swc-starter.git"
  },
  "bugs": {
    "url": "https://github.com/maxgfr/typescript-swc-starter/issues"
  },
  "homepage": "https://github.com/maxgfr/typescript-swc-starter#readme",
  "files": [
    "dist"
  ],
  "keywords": [
    "boilerplate",
    "typescript",
    "release",
    "swc"
  ],
  "scripts": {
    "prepare": "husky",
    "start:cjs": "node ./dist/index.cjs",
    "start:esm": "node ./dist/index.mjs",
    "dev": "nodemon",
    "develop": "node --import @swc-node/register/esm-register --enable-source-maps ./src/index.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "clean": "rimraf build && rimraf dist",
    "build": "swc ./src -d build",
    "build:watch": "swc ./src -d build -w",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "check": "biome check ./src",
    "check:fix": "biome check --write ./src",
    "type-check": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "release": "semantic-release",
    "bundle": "unbuild"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@swc-node/register": "^1.11.1",
    "@swc/cli": "0.7.9",
    "@swc/core": "1.15.7",
    "@swc/jest": "0.2.39",
    "@types/jest": "30.0.0",
    "@types/node": "25.0.3",
    "husky": "^9.1.7",
    "jest": "30.2.0",
    "lint-staged": "^16.2.7",
    "nodemon": "3.1.11",
    "rimraf": "6.1.2",
    "semantic-release": "^25.0.2",
    "typescript": "5.9.3",
    "unbuild": "^3.6.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@swc/core",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
